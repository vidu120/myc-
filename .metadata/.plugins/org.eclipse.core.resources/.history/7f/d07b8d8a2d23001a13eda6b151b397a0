#include <iostream>
#include<string>
using namespace std;

int main() {
	int n;
	cin >> n;
	while (n--) {
		int yr, m, dd;
		string y;
		cin >> y;

		yr = stoi(y.substr(0, 4));
		m = stoi(y.substr(5, 6));
		dd = stoi(y.substr(8, 9));
		int a[2][12] = { { 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 }, {
				1, 0, 1, 0, 1, 0, 1, 1, 0, 1, 0, 1 } };
		if ((yr % 4 == 0 & yr % 100 != 0)
				|| (yr % 4 == 0 & yr % 100 == 0 & yr % 400 == 0)) {
			a[0][1] = 29;
			a[1][1] = 1;
		}
		int ans = 0;
		if (a[1][m - 1] == 1) {
			ans = a[0][m - 1] - dd + 1;
			if (dd % 2 == 0) {
				ans = ans / 2;
			} else {
				ans = (ans + 1) / 2;
			}
		} else {
			ans = a[0][m - 1] - dd + 1;
			if (dd % 2 != 0) {
				ans = ans / 2;
				ans = ans + 15;
			} else {
				ans = (ans + 1) / 2;
				ans = ans + 15;
			}
		}
		cout << ans << endl;

	}

	return 0;
}
