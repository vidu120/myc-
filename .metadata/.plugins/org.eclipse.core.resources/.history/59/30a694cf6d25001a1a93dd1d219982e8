#include <iostream>
#include<cmath>
using namespace std;

int testcases() {
	int n;
	cin >> n;
	return n;
}

int pinlength() {
	int n;
	cin >> n;
	return n;
}

int main() {
	int n = testcases();
	while (n--) {
		int N = pinlength();
		if (N % 2 != 0) {
			cout << 1 << " " << pow(10, (N - 1) / 2) << endl;
		} else {
			cout << 1 << " " << pow(10, N / 2) << endl;
		}
	}

	return 0;
}
