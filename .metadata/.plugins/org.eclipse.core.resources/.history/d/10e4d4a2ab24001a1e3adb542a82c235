#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	while (n--) {
		int N;
		cin >> N;
		int a[N + 1][2] = { };
		for (int i = 0; i < (N = 1); i++) {
			for (int j = 0; j < 2; j++) {
				cin>>a[i][j];
			}
		}


	}

	return 0;
}
