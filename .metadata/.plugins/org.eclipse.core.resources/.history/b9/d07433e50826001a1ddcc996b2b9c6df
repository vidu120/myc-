#include <bits/stdc++.h>
using namespace std;
int main() {
	int n;
	cin >> n;
	while (n--) {
		int N;
		cin >> N;
		if (N % 2 != 0) {
			cout << "1 1" << flush;
			for (int i = 0; i < (N - 1) / 2; i++) {
				cout << "0" << flush;
			}
			cout << endl;
		} else {
			cout << "1 1" << flush;
			for (int i = 0; i < N / 2; i++) {
				cout << "0" << flush;
			}
			cout << endl;
		}
	}

	return 0;
}
