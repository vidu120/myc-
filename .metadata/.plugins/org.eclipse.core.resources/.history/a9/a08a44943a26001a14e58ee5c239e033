#include <iostream>
#include<cmath>
using namespace std;

int main() {
	double X, Y;
	cin >> X >> Y;
	int a = 0, b = 0, c;
	for (int i = 0; true; i++) {
		c = X / pow(10, i);
		if (c < 1) {
			a = i + 3;
			break;
		}
	}
	for (int i = 0; true; i++) {
		c = Y / pow(10, i);
		if (c < 1) {
			b = i + 3;
			break;
		}
	}
	cout << a << " " << b << endl;

	return 0;
}
