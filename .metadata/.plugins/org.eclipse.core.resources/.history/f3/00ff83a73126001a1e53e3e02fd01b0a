#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	while (n--) {
		long int a[2] = { };
		cin >> a[0] >> a[1];
		long int b[a[0] + 1] = { };
		for (int i = 1; i < a[0]; i++) {
			cin >> b[i];
		}
		cout<<b[0]<<endl;

	}
	return 0;
}
