#include <iostream>
#include <cmath>
using namespace std;

int main() {
	long long int n;
	cin >> n;
	for (long long int i = 0; i < n; i++) {
		long long int N;
		cin >> N;
		long long int a[N] = { };
		a[0] = 0;
		a[1] = 1;
		for (long long int j = 2; j < N; j++) {
			a[j] = a[j - 2] + a[j - 1];
		}
		for (long long int j = 0; j < N; j++) {
			a[j] = a[j] % 10;
			cout<<a[j]<<" "<<flush;
		}
		//now we have to take only the odd numbers until only one number is left !!
		// Here comes the most important part !!
		long long int c = 0;
		for (long long int j = 1; true; j++) {
			c = N / (pow(2, j));
			if (c == 1) {
				c = j;
				break;
			}
		}
		cout<<c<<endl;
		long long int ans = 0;
		ans = pow(2, c) - 1;
		cout<<ans<<endl;
		cout << a[ans] << endl;
		return 0;
	}
}
