#include <iostream>
#include<string>
using namespace std;

int main() {
	long int n;
	cin >> n;
	while (n--) {
		long int N;
		cin >> N;
		string a[N] = { };
		for (int i = 0; i < N; i++) {
			cin >> a[i];
		}
		long int time[N] = { };
		string test[4] = { "d", "f", "j", "k" };
		int test1[4] = { 0, 0, 1, 1 };
		for (int i = 0; i < N; i++) {
			int b[a[i].size()] = { };
			string pr;
			for (unsigned int j = 0; j < a[i].size(); j++) {
				pr = a[i].substr(j, 1);
				for (int k = 0; k < 4; k++) {
					if (pr == test[k]) {
						b[j] = test1[k];
						break;
					}
				}
			}
			int d = 0;
			if (i != 0) {
				for (int j = 0; j < i; j++) {
					if (a[i] == a[j]) {
						time[i] = time[j] / 2;
						d = j;
						break;
					}
				}
				if (a[i] == a[d]) {
					cout << time[i] << " " << flush;
					continue;
				}
			}
			time[i] = 2;
			for (unsigned int j = 1; j < a[i].size(); j++) {
				if (b[j] == b[j - 1]) {
					time[i] = time[i] + 4;
				} else {
					time[i] = time[i] + 2;
				}
			}
			cout << time[i] << " " << flush;
		}
		cout << endl;
	}

	return 0;
}
